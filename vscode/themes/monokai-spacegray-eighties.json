{
    "name": "Monokai Spacegray Eighties",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        "editor.background": "#1C1C1C",
        "editor.foreground": "#F8F8F8",
        "editor.lineHighlightBackground": "#343434",
        "editor.selectionBackground": "#3C3C3C",
        "editorCursor.foreground": "#F8F8F8",
        "editorWhitespace.foreground": "#3A3A3A"
    },
    "tokenColors": [
        {
            "name": "Variables and Objects",
            "scope": [
                "variable",
                "variable.other",
                "variable.objectsource.r",
                "meta.definition.variable",
                "support.class",
                "support.other",
                "variable.other.constant",
                "entity.name.namespace",
                "support.type.namespace",
                "support.other.namespace"
            ],
            "settings": {
                "foreground": "#F8F8F8"
            }
        },
        {
            "name": "Comments and Documentation",
            "scope": [
                "comment",
                "comment.line",
                "comment.block",
                "comment.documentation.r"
            ],
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#AE81FF"
            }
        },
        {
            "name": "Functions and Methods",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function",
                "meta.method-call",
                "meta.function.call",
                "keyword.operatorsource"
            ],
            "settings": {
                "foreground": "#66D9EF"
            }
        },
        {
            "name": "Keywords and Storage",
            "scope": [
                "keyword",
                "storage",
                "storage.type",
                "storage.modifier",
                "variable.language"
            ],
            "settings": {
                "foreground": "#F92672"
            }
        },
        {
            "name": "Function Parameters",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FD971F"
            }
        }
    ]
}
